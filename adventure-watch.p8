__lua__
function _init()
  paused = true
  reset = false
  init_menu()
end
function _update()
  if reset then
    start_game()
  end
  if paused then
    update_menu()
  else
    if (btnp(5,pad)) then paused = not paused end
    update_sim()
    update_mini()
  end
end
function _draw()
  if paused then
    draw_menu()
  else
    draw_sim()
    draw_mini()
  end
end
function start_game()
  play_bg_music(isMusic)
  init_sim()
  init_mini()
  pause = false
  reset = false
end
